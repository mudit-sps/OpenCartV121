<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Sanity Suite">
	
	<groups>
		<run>
			<!--include name="Master"></include-->
			<include name="Sanity"></include>
			<!--<include name="Regression"></include>-->
			<!--exclude name="DataDriven"></exclude-->
		</run>
	</groups>
	<listeners>
		<listener class-name="utilities.ExtentsReportManager"></listener>
	</listeners>
	<test name="SanityTest" thread-count="3">
		<parameter name="Browser" value="Chrome"></parameter>
		<parameter name="OS" value="Windows"></parameter>
		<classes>
			<class name="testCases.TC_001AccountRegistration"/>
			<class name="testCases.TC_002LoginTest"></class>
     		<class name="testCases.TC003_LoginDDT"></class>
		</classes>		
	</test>
</suite>